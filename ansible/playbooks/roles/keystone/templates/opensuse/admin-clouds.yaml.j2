# {{ ansible_managed }}

clouds:
  local:
    auth:
      username: "admin"
      password: "{{ keystone_admin_password }}"
      project_name: "admin"
      auth_url: "{% if stack_ssl_enabled %}https{% else %}http{% endif %}://{{ stack_management_ipaddress }}:35357/v3"
      user_domain_id: "default"
      project_domain_id: "default"
    region_name: "{{ keystone_region_name }}"
    identity_api_version: "3"
    image_api_version: "2"
    cacert: "/etc/ssl/ca-bundle.pem"
